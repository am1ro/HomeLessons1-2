<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">
    <TextView
            android:text="@string/textview"
            android:layout_width="398dp"
            android:layout_height="39dp" android:id="@+id/textView" tools:layout_editor_absoluteY="183dp"
            tools:layout_editor_absoluteX="16dp" tools:ignore="MissingConstraints"/>
    <EditText
            android:layout_width="389dp"
            android:layout_height="40dp"
            android:inputType="textPersonName"
            android:text="@string/name"
            android:ems="10"
            android:id="@+id/editTextTextPersonName"
            tools:layout_editor_absoluteX="16dp" app:layout_constraintTop_toBottomOf="@+id/textView"
            android:textAlignment="viewStart" tools:ignore="LabelFor,MissingConstraints" android:autofillHints=""/>
    <EditText
            android:layout_width="383dp"
            android:layout_height="36dp"
            android:inputType="phone"
            android:ems="10"
            android:id="@+id/editTextPhone" tools:layout_editor_absoluteX="16dp"
            app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName" tools:ignore="LabelFor,MissingConstraints"
            android:autofillHints=""/>
    <EditText
            android:layout_width="377dp"
            android:layout_height="42dp"
            android:inputType="number"
            android:ems="10"
            android:id="@+id/editTextNumber"
            tools:layout_editor_absoluteX="16dp" app:layout_constraintTop_toBottomOf="@+id/editTextPhone"
            tools:ignore="LabelFor,MissingConstraints" android:autofillHints=""/>
    <EditText
            android:layout_width="374dp"
            android:layout_height="56dp"
            android:inputType="textPassword"
            android:ems="10"
            android:id="@+id/editTextTextPassword"
            tools:layout_editor_absoluteX="16dp" app:layout_constraintTop_toBottomOf="@+id/editTextNumber"
            tools:ignore="LabelFor,MissingConstraints" android:autofillHints=""/>
    <EditText
            android:layout_width="380dp"
            android:layout_height="52dp"
            android:inputType="textEmailAddress"
            android:ems="10"
            android:id="@+id/editTextTextEmailAddress" tools:layout_editor_absoluteY="399dp"
            tools:layout_editor_absoluteX="16dp" tools:ignore="LabelFor,MissingConstraints" android:autofillHints=""/>
    <CalendarView
            android:layout_width="205dp"
            android:layout_height="287dp" android:id="@+id/calendarView"
            tools:layout_editor_absoluteX="49dp"
            tools:ignore="MissingConstraints" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="64dp"/>

</androidx.constraintlayout.widget.ConstraintLayout>